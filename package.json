{
  "name": "stl",
  "description": "stl",
  "version": "1.0.0",
  "scripts": {
    "start": "nodemon",
    "build:local": "cross-env APP_ENV=local next build && tsc --project tsconfig.server.json",
    "start:local": "cross-env APP_ENV=local node dist/index.js",
    "build:dev": "cross-env APP_ENV=dev next build && tsc --project tsconfig.server.json",
    "start:dev": "cross-env APP_ENV=dev node dist/index.js",
    "build:stage": "cross-env APP_ENV=stage next build && tsc --project tsconfig.server.json",
    "start:stage": "cross-env APP_ENV=stage node dist/index.js",
    "build": "next build && tsc --project tsconfig.server.json",
    "test": "jest --coverage",
    "lint": "eslint ./{src,pages,server}/**/* --ext .tsx,.ts",
    "pretty": "prettier --write --config .prettierrc.json {src,pages,server}/**/*.{ts,tsx}",
    "format": "npm run lint -- --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "export": "cross-env APP_ENV=production next export",
    "cli": "pankod-cli add",
    "storybook": "start-storybook -s ./public/static -p 6006",
    "build-storybook": "build-storybook -s ./public/static  -c .storybook -o .storybookout",
    "sonar": "node sonar-project.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh check_sh.sh && npm run lint"
    }
  },
